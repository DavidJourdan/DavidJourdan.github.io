{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch index.html",
            "sourceMaps": false,
            "file": "${workspaceRoot}/index.html",
            "runtimeExecutable": "/usr/bin/chromium"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}